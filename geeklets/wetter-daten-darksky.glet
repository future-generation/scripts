	<key>Command</key>
	<string>#!/bin/bash
# Needs command-line JSON processor: brew install jq
## Variablen setzen
export PATH=$PATH:/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin
output_json="/tmp/weather-data.json"
output_png="/tmp/weather.png"
# Wetter Daten lokal als JSON speichern
curl --silent --compressed -H "Accept-Encoding: gzip,deflate" "https://api.darksky.net/forecast/[FILL_IN_DARKSKY_API_KEY]/48.7784,9.18?lang=de&amp;units=auto&amp;exclude=hourly,flags" -o $output_json
# Gespeicherte JSON Daten parsen und alle Infos raus poppeln und darstellen
printf %b "\e[1m\e[4mAktuelles Wetter vom $(date '+%d.%m.%Y um %H:%M')\e[0m\n"
printf %b "$(cat $output_json | jq ".currently.summary" | tr -d '"'), $(cat $output_json |jq ".currently.temperature" | sed 's/\..*//')\xc2\xb0C\n\n"
printf %b "\e[1m\e[4mVorhersage für heute:\e[0m\n"
printf %b "$(cat $output_json | jq ".daily.data[0].summary" | tr -d '"') Tiefsttemperatur: $(cat $output_json | jq ".daily.data[0].temperatureLow" | sed 's/\..*//')\xc2\xb0C, Höchsttemperatur: $(cat $output_json | jq ".daily.data[0].temperatureHigh" | sed 's/\..*//')\xc2\xb0C\n\n"
printf %b "\e[1m\e[4mVorhersage für morgen:\e[0m\n"
printf %b "$(cat $output_json | jq ".daily.data[1].summary" | tr -d '"') Tiefsttemperatur: $(cat $output_json | jq ".daily.data[1].temperatureLow" | sed 's/\..*//')\xc2\xb0C, Höchsttemperatur: $(cat $output_json | jq ".daily.data[1].temperatureHigh" | sed 's/\..*//')\xc2\xb0C\n\n"
printf %b "\e[1m\e[4mFeinstaub Status:\e[0m\n"
feinstaub_title=$(curl --silent --compressed -H "Accept-Encoding: gzip,deflate" https://www.stuttgart.de/feinstaubalarm | pup 'div[class="message"] h1 text{}')
if [[ $feinstaub_title = "FEINSTAUBALARM IN STUTTGART" ]]; then
	printf %b "Feinstaubalarm in Stuttgart ausgerufen"
else
	printf %b "Aktuell kein Feinstaubalarm"
fi</string>
