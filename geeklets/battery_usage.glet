	<key>Command</key>
	<string>maxcap=$(ioreg -l | grep Capacity |awk '/"MaxCapacity"/{print $5}')
curcap=$(ioreg -l | grep Capacity |awk '/"CurrentCapacity"/{print $5}')
curcap=$(echo $curcap*1000|bc)
proc=$(echo $curcap / $maxcap |bc)
len=$(echo ${#proc})
proc_num=${proc:0:3}
proc_dec=${proc:3:4}
 
proc_num_len=`echo ${#proc_num}`;
 
if [ $proc_num_len == "3" ]; then
        if [ $proc_num != "100" ]; then
                proc_num=${proc:0:2}
                proc_dec=${proc:2:3}
        fi
fi
 
echo "\033[0m\nMacBook Pro Battery:\t\c"

BatteryPercent=$proc_num

if [ ${#BatteryPercent} = 0 ]
then
    echo "Disconnected\c"
else
    if [ $BatteryPercent -lt 11 ]; then
        echo "\033[1;31m\c"
    else
        echo "\033[0m\c"
    fi

    typeset -i b=5
    while [ $b -le $BatteryPercent ]; do
        echo "|\c"
        b=$(expr $b + 5)
    done

    while [ $b -le 100 ]; do
        echo "\033[1;37m|\033[0m\c"
        b=$(expr $b + 5)
    done

    echo "\033[0m $BatteryPercent%\c"

    unset BatteryPercent
    unset b
fi</string>
